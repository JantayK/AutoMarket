@model GetBrandModelGenerationDto

@{
    ViewData["Title"] = "Выбор модели";
}

<div class="form-row">
    <div id="AdvertFormModel">
        <form class="form-group" style="margin-right:28px">
            <select class="custom-select" id="selectModelId" style="width: 240px; border-radius: 15px">
                <option disabled selected>Выберите модель</option>
                @foreach (var item in Model.GetModelsDto)
                {
                    <option value="@item.Id" class="form-control">@item.Name</option>
                }
            </select>
        </form>
    </div>
</div>